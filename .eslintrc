{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/recommended",
    "prettier"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "prettier/prettier": ["error", {"singleQuote": true, "trailingComma": "es5"}],
    "prefer-const": 2
  },
  "plugins": [
    "import",
    "react",
    "prettier"
  ]
}